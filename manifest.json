{
	"name": "Meet Statistics",
	"description": "Track user engagement in Google Meet by monitoring attendance, text messages, reactions, hands up and camera/voice activity",
	"version": "1.0.0",
	"manifest_version": 3,
	"action": {
		"default_popup": "popup/index.html",
		"default_icon": {
			"16": "main/img/16.png",
			"24": "main/img/24.png",
			"32": "main/img/32.png",
			"48": "main/img/48.png",
			"64": "main/img/64.png",
			"128": "main/img/128.png"
		}
	},
	"options_page": "main/index.html",
	"permissions": ["storage", "unlimitedStorage", "activeTab"],
	"content_scripts": [
		{
			"js": [
				"content/main.js"
			],
			"matches": [
				"*://meet.google.com/*-*-*"
			],
			"run_at": "document_start",
			"world": "MAIN"
		},
		{
			"js": [
				"content/decoder.js",
				"utils/Store.js",
				"utils/Utils.js",
				"content/isolate.js"
			],
			"matches": [
				"*://meet.google.com/*-*-*"
			],
			"run_at": "document_start"
		}
	],
	"icons": {
		"16": "main/img/16.png",
		"24": "main/img/24.png",
		"32": "main/img/32.png",
		"48": "main/img/48.png",
		"64": "main/img/64.png",
		"128": "main/img/128.png"
	},
	"author": {
		"email": "timotejroiko@yahoo.com.br"
	},
	"homepage_url": "https://github.com/timotejroiko/meet-statistics",
	"minimum_chrome_version": "88",
	"key": ""
}
